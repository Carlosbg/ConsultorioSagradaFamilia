<Page x:Class="SagradaFamilia3._0.Windows.Views.Reportes.ReportesIndex"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SagradaFamilia3._0.Windows.Views.Reportes"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ReportesIndex">

    <Grid>
        <DatePicker Name="FechaDesde" HorizontalAlignment="Left" Margin="110,35,0,0" VerticalAlignment="Top" Width="162"/>
        <DatePicker Name="FechaHasta" HorizontalAlignment="Left" Margin="361,35,0,0" VerticalAlignment="Top" Width="173"/>
        <Label Content="Fecha Desde" HorizontalAlignment="Left" Margin="26,35,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.53,0.326" Width="79"/>
        <Label Content="Fecha Hasta" HorizontalAlignment="Left" Margin="277,35,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.53,0.326" Width="79"/>
        <Button Name="Ganancias" Content="Ganancias" HorizontalAlignment="Left" Margin="26,128,0,0" VerticalAlignment="Top" Width="197" Click="Ganancias_Click"/>
        <Button x:Name="ObrasSocialesHabilitadas" Content="Obras Sociales Habilitadas" HorizontalAlignment="Left" Margin="26,173,0,0" VerticalAlignment="Top" Width="197" Click="ObrasSocialesHabilitadas_Click"/>
