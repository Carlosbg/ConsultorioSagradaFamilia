        <ComboBox Name="MedicosCombobox" HorizontalAlignment="Left" Margin="110,78,0,0" VerticalAlignment="Top" Width="162"/>
        <Label Name="MedicoLabel" Content="Médico" HorizontalAlignment="Left" Margin="26,78,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.53,0.326" Width="79"/>
        <Button x:Name="TurnosPorMedico" Content="Turnos Por Médico" HorizontalAlignment="Left" Margin="26,260,0,0" VerticalAlignment="Top" Width="197" Click="TurnosPorMedico_Click"/>
        <Button x:Name="PacientesEnEspera" Content="Pacientes En Espera" HorizontalAlignment="Left" Margin="26,303,0,0" VerticalAlignment="Top" Width="197" Click="PacientesEnEspera_Click"/>
        <Button x:Name="GananciasGenerales" Content="Ganancias Generales" HorizontalAlignment="Left" Margin="236,128,0,0" VerticalAlignment="Top" Width="197" Click="GananciasGenerales_Click"/>
    </Grid>
