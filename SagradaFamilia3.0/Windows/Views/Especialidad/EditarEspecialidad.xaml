<Page x:Class="SagradaFamilia3._0.Windows.Views.Especialidad.EditarEspecialidad"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SagradaFamilia3._0.Windows.Views.Especialidad"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditarEspecialidad">

    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="Nombre" Height="14" Margin="190,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="203" />
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="51,12,0,0" VerticalAlignment="Top" Width="121"/>
        <Button Content="Volver" HorizontalAlignment="Left" Margin="62,102,0,0" VerticalAlignment="Top" Width="75" Click="ButtonVolver_Click"/>
        <Button Content="Editar" HorizontalAlignment="Left" Margin="318,102,0,0" VerticalAlignment="Top" Width="75" Click="ButtonEditar_Click"/>
        <CheckBox x:Name="Habilitada" Content="&#xA;" HorizontalAlignment="Left" Margin="188,53,0,0" VerticalAlignment="Top"/>
        <Label Content="Habilitada" HorizontalAlignment="Left" Margin="51,53,0,0" VerticalAlignment="Top" Width="121"/>

    </Grid>
</Page>
